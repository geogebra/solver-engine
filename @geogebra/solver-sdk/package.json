{
  "name": "@geogebra/solver-sdk",
  "version": "3.14.0",
  "description": "Provides a convenient interface for the GGB Solver API.",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/esm/index.d.ts",
  "files": [
    "lib",
    "dist",
    "src",
    "test"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs && npm run build:web",
    "build:esm": "tsc",
    "build:cjs": "tsc -p ./tsconfig-cjs.json",
    "build:web": "vite build",
    "watch": "tsc --watch",
    "start": "vite --host",
    "test": "nyc ../../node_modules/.bin/_mocha 'test/**/*.test.ts'",
    "test-watch": "nodemon -e * --exec ../../node_modules/.bin/_mocha 'test/**/*.test.ts'",
    "test-watch-full": "nodemon -e * --exec npm run test",
    "semantic-release": "semantic-release",
    "prettier": "cd ../../ && prettier -w ."
  },
  "publishConfig": {
    "access": "restricted"
  },
  "contributors": [
    "Erik Weitnauer <erik.weitnauer@geogebra.at>"
  ],
  "license": "ISC",
  "volta": {
    "node": "18.15.0"
  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/gitlab": "^12.0.0",
    "@semantic-release/npm": "^9.0.2",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "cross-fetch": "^3.1.5",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "node-fetch": "^3.3.0",
    "prettier": "2.8.4",
    "semantic-release": "^20.1.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "ts-node": "^10.9.1"
  }
}
